{
  "hosting": {
    "source": ".",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "frameworksBackend": {
      "region": "us-central1"
    },
    "rewrites": [
      { "source": "/_next/**", "function": "ssrfunction" },
      { "source": "/public/**", "destination": "/public/$1" },
      { "source": "/**", "function": "ssrfunction" }
    ]
  }
}
